 setQuerySort] = useState(\"name\");\r\n\r\n\r\n\r\n    const sort = [\r\n        {\r\n            label: \"Name\",\r\n            value: \"name\",\r\n        },\r\n        {\r\n            label: \"Added\",\r\n            value: \"created_at\",\r\n        },\r\n    ];\r\n    const order = [\r\n        {\r\n            label: \"Ascending\",\r\n            value: \"ASC\",\r\n        },\r\n        {\r\n            label: \"Descending\",\r\n            value: \"DESC\",\r\n        },\r\n    ];\r\n    const limit = [\r\n        {\r\n            label: \"Limit 1\",\r\n            value: \"1\",\r\n        },\r\n        {\r\n            label: \"Limit 2\",\r\n            value: \"2\",\r\n        },\r\n        {\r\n            label: \"Limit 4\",\r\n            value: \"4\",\r\n        },\r\n        {\r\n            label: \"Limit 5\",\r\n            value: \"5\",\r\n        },\r\n    ];\r\n\r\n\r\n\r\n\r\n\r\n    const handleClickPage = (index) => {\r\n        setPage(index + 1);\r\n    };\r\n\r\n    let token;\r\n    if (typeof window !== \"undefined\") {\r\n        token = localStorage.getItem(\"token\");\r\n    }\r\n\r\n    useEffect(() => {\r\n        const id = localStorage.getItem('id')\r\n        // http://localhost:8080/api/v1/transaction/user/7?page=1&perPage=2&sortBy=createdAt&order=asc\r\n        const url = `${process.env.api}/transaction/user/${id}?page=${page}&perPage=${queryLimit}`;\r\n        // console.log(url);\r\n        axios.get(url, {\r\n            headers: {\r\n                Authorization: 'Bearer ' + token\r\n            }\r\n        })\r\n            .then((res) => {\r\n                const data = res.data.data\r\n                // console.log(res.data);\r\n                setUser(data)\r\n                setTotalPage(res.data.totalPage)\r\n                setCurrentPage(res.data.currentPage)\r\n            })\r\n            .catch((err) => {\r\n                console.log(err);\r\n            })\r\n    }, [page]);\r\n\r\n\r\n    return (\r\n        <div className={style['card-history']}>\r\n            <h3>Transaction History</h3>\r\n            {user.map((item, index) => {\r\n                return (\r\n                    <div className={[['d-flex'], ['mb-3'], ['justify-content-between'], style['card-transaction-history']].join(\" \")} key={index}>\r\n                        <img src={`${process.env.api_img}${item.image}`}\r\n                            alt=\"Picture Transaction\"\r\n                            width={70}\r\n                            height={70} />\r\n                        <div className={style['text-transaction']} >\r\n                            <h5>{item.firstname} {item.lastname}</h5>\r\n                            <p>{item.type}</p>\r\n                        </div>\r\n                        <div>\r\n                            <h6 className={`${item.type === \"Transfer\" ? style['amount-transfer'] : style['amount-receiver']}`} >\r\n                                {\r\n                                    item.type === \"Transfer\"\r\n                                        ? `-Rp${item.amount}`\r\n                                        : `+Rp${item.amount}`\r\n                                }\r\n                            </h6>\r\n                        </div>\r\n                    </div>\r\n                );\r\n            })}\r\n            {/* Paginasi */}\r\n            <div className=\"row pl-2 pl-lg-0 mt-5\">\r\n                <div className=\"col-12 d-flex justify-content-center\">\r\n                    {parseInt(totalPage) > 1 ? (\r\n                        <Pagination aria-label=\"Page navigation example\">\r\n                            <PaginationItem>\r\n                                <PaginationLink first onClick={(e) => setPage(1)} />\r\n                            </PaginationItem>\r\n                            {Array.from(Array(totalPage).keys()).map((data, index) => {\r\n                                return (\r\n                                    <PaginationItem active={currentPage === index + 1}>\r\n                                        <PaginationLink onClick={(e) => handleClickPage(index)}>\r\n                                            {index + 1}\r\n                                        </PaginationLink>\r\n                                    </PaginationItem>\r\n                                );\r\n                            })}\r\n                            <PaginationItem>\r\n                                <PaginationLink\r\n                                    last\r\n                                    onClick={(e) => setPage(totalPage)}\r\n                                />\r\n                            </PaginationItem>\r\n                        </Pagination>\r\n                    ) : (\r\n                        \"\"\r\n                    )}\r\n                </div>\r\n            </div>\r\n            {/* Akhir Pagnisai */}\r\n            {/* awal sorting */}\r\n            <div className=\"row mt-5 justify-content-center\">\r\n                <div className=\"col-3\">\r\n                    <select\r\n                        onChange={(event) => setQuerySort(event.target.value)}\r\n                        className=\"w-100 custom-select font-weight-normal\"\r\n                    >\r\n                        {sort.map((item, index) => {\r\n                            return (\r\n                                <option value={item.value} key={index}>\r\n                                    {item.label}\r\n                                </option>\r\n                            );\r\n                        })}\r\n                    </select>\r\n                </div>\r\n                <div className=\"col-3\">\r\n                    <select\r\n                        onChange={(event) => {\r\n                            setQueryOrder(event.target.value);\r\n                        }}\r\n                        className=\"w-100 custom-select\"\r\n                    >\r\n                        {order.map((item, index) => {\r\n                            return (\r\n                                <option value={item.value} key={index}>\r\n                                    {item.label}\r\n                                </option>\r\n                            );\r\n                        })}\r\n                    </select>\r\n                </div>\r\n                <div className=\"col-3\">\r\n                    <select\r\n                        onChange={(event) => {\r\n                            setQueryLimit(event.target.value);\r\n                        }}\r\n                        className=\"w-100 custom-select\"\r\n                    >\r\n                        {limit.map((item, index) => {\r\n                            return (\r\n                                <option value={item.value} key={index}>\r\n                                    {item.label}\r\n                                </option>\r\n                            );\r\n                        })}\r\n                    </select>\r\n                </div>\r\n            </div>\r\n            {/* akkhir sorting */}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default HistoryParts\r\n"]},"metadata":{},"sourceType":"module"}